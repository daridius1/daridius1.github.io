---
import mapChile from "../../assets/map_chile_rotated.svg";
import calcBg from "../../assets/calc_bg.jpg";
import EleccionesApp from "../../components/elecciones/EleccionesApp";
import "../../styles/global.css";
---

<!doctype html>
<html lang="es">
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
        <title>Elecciones</title>
        <style>
            body,
            html {
                margin: 0;
                padding: 0;
                height: 100dvh;
                overflow: hidden;
            }
        </style>
    </head>
    <body>
        <EleccionesApp
            client:only="react"
            mapSrc={mapChile.src}
            bgSrc={calcBg.src}
        />
    </body>
</html>
